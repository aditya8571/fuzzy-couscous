#include<stdio.h>
int get_input()
{
        int a;
        printf("enter a number\n");
        scanf("%d",&a);
        return a;
}
int add_one(int a)
{
        int b;
        b=a+1 ;
        return b;
}
void output(int a,int b)
{
        printf("%d + 1 is %d",a,b);
}
int main()
{
        int a,b;

        a=get_input();

        b=add_one(a);

        output(a,b);
}
